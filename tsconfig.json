{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "stories/*": ["src/stories/*"],
      "img/*": ["src/img/*"],
      "style/*": ["src/style/*"],
      "store/*": ["src/store/*"],
      "storybook/*": [".storybook/*"]
    }
  }
}
