{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "stories/*": ["src/stories/*"],
      "img/*": ["src/img/*"],
      "theme/*": ["src/theme/*"],
      "store/*": ["src/store/*"],
      "storybook/*": [".storybook/*"],
      "data/*": ["src/data/*"],
      "navigation/*": ["src/navigation/*"],
      "root/*": ["./*"]
    }
  }
}
